{
 "manifest_version": 2,
 "name": "Credman",
 "version": "1.0.0",
 "description": "Password Manager",
 "short_name": "Short Sample Name",
 "permissions": ["activeTab", "<all_urls>"],
 "content_scripts": [
   {
     "matches": ["<all_urls>"],
     "css": ["background.css"],
     "js": ["background.js"]
   }
 ],
 "browser_action": {
   "default_title": "Saves Password to credman api",
   "default_popup": "popup.html",
   "default_icon": {
     "16": "icons/icon16.png",
   }
 }
}
